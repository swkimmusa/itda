{
  "name": "pacto-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "1.8.2",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "13.5.0",
    "antd": "5.1.6",
    "classnames": "2.3.1",
    "currency.js": "2.0.4",
    "dayjs": "1.11.7",
    "formik": "2.2.9",
    "isomorphic-fetch": "3.0.0",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "number-currency-format": "1.0.10",
    "prop-types": "15.8.1",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.2",
    "react-refresh": "0.11.0",
    "react-router-dom": "6.3.0",
    "react-router-prop-types": "1.0.5",
    "react-scripts": "5.0.1",
    "redux": "4.2.0",
    "redux-persist": "6.0.0",
    "redux-saga": "1.2.1",
    "styled-components": "5.3.5",
    "styled-theme": "0.3.3",
    "styled-tools": "1.7.2",
    "uuid": "9.0.0",
    "web-vitals": "2.1.4",
    "web3": "1.7.3",
    "webpack": "5.74.0",
    "workbox-background-sync": "6.5.4",
    "workbox-broadcast-update": "6.5.4",
    "workbox-cacheable-response": "6.5.4",
    "workbox-core": "6.5.4",
    "workbox-expiration": "6.5.4",
    "workbox-google-analytics": "6.5.4",
    "workbox-navigation-preload": "6.5.4",
    "workbox-precaching": "6.5.4",
    "workbox-range-requests": "6.5.4",
    "workbox-routing": "6.5.4",
    "workbox-strategies": "6.5.4",
    "workbox-streams": "6.5.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:page": "NODE_ENV=development react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "storybook:clear-cache": "start-storybook --no-manager-cache",
    "build-storybook": "build-storybook -s public",

    "predeploy:page": "npm run build:page",
    "deploy:page": "gh-pages -d build",

    "predeploy:staging": "REACT_APP_STAGE=staging npm run build",
    "deploy:staging": "aws s3 sync build/ s3://itda-app-staging --profile itda",
    "postdeploy:staging": "aws cloudfront create-invalidation --distribution-id E3SDRUWBXMBQZN --paths '/*' --profile itda",

    "predeploy": "REACT_APP_STAGE=production npm run build",
    "deploy": "aws s3 sync build/ s3://itda-app-production --profile itda",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id E3SDRUWBXMBQZN --paths '/*' --profile itda"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-interactions": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/builder-webpack5": "6.5.9",
    "@storybook/manager-webpack5": "6.5.9",
    "@storybook/node-logger": "6.5.9",
    "@storybook/preset-create-react-app": "4.1.2",
    "@storybook/react": "6.5.9",
    "@storybook/testing-library": "0.0.13",
    "babel-plugin-named-exports-order": "0.0.2",
    "eslint": "8.18.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-destructuring-newline": "0.0.2",
    "gh-pages": "4.0.0",
    "sass": "1.53.0",
    "storybook-addon-styled-component-theme": "2.0.0"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
